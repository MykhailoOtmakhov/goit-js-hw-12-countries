{{#each this}}
<div class="card">
    <div class="card-img">
        <img src={{flag}} alt="{{name}}" class="card-img" width="inherit">
    </div>
    <div class="card-body">
        <h2 class="card-title">Country: {{name}}</h2>
        <p class="card-text">Capital: {{capital}} </p>
        <p class="card-text">Population: {{population}}</p>
        <ul class="list">Languages:</ul>
            {{#each languages}}
                <li class="item">{{name}}</li>
            {{/each}}
    </div>
</div>
{{/each}}
